{
    "functions": {
      "server.js": {
        "memory": 1024,
        "includeFiles": "data/cities.json"
      }
    },
    "rewrites": [
      {
        "source": "^/api/(.*)",
        "destination": "/server.js"
      },
      {
        "source": "^/([^/]+)?",
        "destination": "/index.html"
      }
    ]
  }
  